<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6.4//EN" 
        "http://google-web-toolkit.googlecode.com/svn/tags/1.6.4/distro-source/core/src/gwt-module.dtd">

<module rename-to='app'>

    <!-- Google GIN -->
    <inherits name="com.google.gwt.inject.Inject"/>     <!-- Dependency Injection -->

    <inherits name='com.google.gwt.user.User'/>         <!-- Core GWT      -->
    <inherits name="com.google.gwt.jsonp.Jsonp" />      <!-- JSONP Requests -->
    <inherits name="com.google.gwt.json.JSON" />        <!-- Parsing JSON  -->

    <!-- SmartGWT Mobile Framework                    -->
    <inherits name="com.smartgwt.mobile.SmartGwtMobile"/>


    <!-- Comment out next line for all target browsers; safari, gecko1_8, ie6, ie8, opera   -->
    <set-property name="user.agent" value="safari"/>

    <!-- Specify the app entry point class.                         -->
    <entry-point class='org.informagen.mobileeo.client.EntryPoint'/>
    
    <source path='client'/>
    <source path='jso'/>
    <source path='service'/>
   
</module>
