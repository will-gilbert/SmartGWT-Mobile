<?xml version="1.0" encoding="UTF-8"?>
<!--
This GWT module is used to compile the Showcase app for use with PhoneGap / Apache Cordova
on Android.
-->
<module rename-to="showcase">
    <inherits name="com.smartgwt.mobile.showcase.Showcase"/>
    <set-property name="user.agent" value="android"/>
    <set-property name="isc.usingCordova" value="yes"/>
    <!-- <set-property name="android.windowSoftInputMode" value="adjustPan"/> -->
</module>
