<?xml version="1.0" encoding="UTF-8"?>

<web-app>

    <display-name>@title@, v@version@</display-name>


    <servlet>
      <servlet-name>countriesDataProvider</servlet-name>
      <servlet-class>com.smartgwt.mobile.showcase.servlet.CountriesDataProviderServlet</servlet-class>
    </servlet>

    <servlet-mapping>
      <servlet-name>countriesDataProvider</servlet-name>
      <url-pattern>/countriesDataProvider</url-pattern>
    </servlet-mapping>

    <!-- General config -->
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>

    <mime-mapping>
        <extension>manifest</extension>
        <mime-type>text/cache-manifest</mime-type>
    </mime-mapping>

    <!-- Default page to serve -->
    <welcome-file-list>
        <welcome-file>@gwtStartPage@</welcome-file>
    </welcome-file-list>

</web-app>
